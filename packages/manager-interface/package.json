{
  "name": "@melonproject/manager-interface",
  "description": "Melon interface",
  "homepage": "http://melonport.com",
  "version": "0.8.4",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "prebuild": "yarn clean",
    "prebuild:app": "yarn clean",
    "build": "cross-env NODE_ENV=production next build src",
    "build:app": "cross-env NODE_ENV=production cross-env ELECTRON=true backpack build && cross-env NODE_ENV=production cross-env ELECTRON=true next build src && cross-env NODE_ENV=production cross-env ELECTRON=true next export src -o app/renderer && electron-builder",
    "start": "cross-env NODE_ENV=production node server.js",
    "predev": "yarn clean",
    "predev:app": "yarn clean",
    "dev": "cross-env NODE_ENV=development node server.js",
    "dev:app": "cross-env NODE_ENV=development cross-env ELECTRON=true backpack dev",
    "clean": "rimraf coverage build",
    "format": "prettier --write \"{src,tests}/**/*.{ts,tsx}\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest"
  },
  "author": "Melonport <team@melonport.com>",
  "contributors": [
    "Simon Emanuel Schmid <simon@schmid.io>",
    "Sebastian Siemssen <fubhy@fubhy.com>",
