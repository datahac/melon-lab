{
  "name": "manager-interface",
  "description": "Melon interface",
  "homepage": "http://melonport.com",
  "version": "1.0.0-alpha.1",
  "private": true,
  "license": "AGPL-3.0",
  "main": "main/index.js",
  "engines": {
    "node": "^8.15.0"
  },
  "scripts": {
    "build:app": "build",
    "build:main": "node scripts build main",
    "build:renderer": "node scripts build renderer",
    "build": "yarn build:main && yarn build:renderer && yarn build:app",
    "clean": "rimraf coverage build",
    "clean:logs": "rimraf logs/*.log",
    "dev": "node scripts dev",
    "format": "prettier --write \"{main,renderer,tests}/**/*.{ts,tsx}\"",
    "lint:integration": "tslint --project tests/tsconfig.json -c tslint.json",
    "lint:main": "tslint --project main/tsconfig.json -c tslint.json",
    "lint:renderer": "tslint --project renderer/tsconfig.json -c tslint.json",
    "lint": "yarn lint:main && yarn lint:renderer && yarn lint:integration",
    "postinstall": "ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true electron-builder install-app-deps",
    "prebuild:app": "rimraf package",
    "prebuild:main": "rimraf build/main",
    "prebuild:renderer": "rimraf build/renderer renderer/.next",
    "prebuild": "rimraf build",
    "storybook": "start-storybook -c renderer/storybook/config --port 3060 --quiet",
    "test:integration": "jest -c tests/jest.config.js --runInBand --forceExit",
    "test:main": "jest -c main/jest.config.js",
    "test:renderer": "jest -c renderer/jest.config.js",
    "test": "yarn test:main && yarn test:renderer && yarn test:integration"
  },
  "author": "Melonport <team@melonport.com>",
  "contributors": [
    "Simon Emanuel Schmid <simon@schmid.io>",
    "Sebastian Siemssen <fubhy@fubhy.com>",
